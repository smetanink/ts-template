{
  "name": "@smetanink/ts-template",
  "version": "1.0.0",
  "description": "Typescript template",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write src",
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "start": "env-cmd ts-node src/index.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "update:dependency:versions": "ncu -u"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smetanink/ts-template"
  },
  "author": "Konstantin Smetanin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smetanink/ts-template/issues"
  },
  "homepage": "https://github.com/smetanink/ts-template#readme",
  "devDependencies": {
    "@types/jest": "29.5.8",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "@vercel/ncc": "0.38.1",
    "env-cmd": "10.1.0",
    "eslint": "8.53.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "15.1.0",
    "prettier": "3.1.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "npm-check-updates": "16.14.6"
  },
  "dependencies": {
    "pino": "8.16.1",
    "typeorm": "0.3.17",
    "pg": "8.11.3"
  }
}

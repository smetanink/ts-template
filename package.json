{
  "name": "@smetanink/ts-template",
  "version": "1.0.0",
  "description": "Typescript template",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write src",
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "start": "env-cmd ts-node src/index.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "update:dependency:versions": "ncu -u"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smetanink/ts-template"
  },
  "author": "Konstantin Smetanin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smetanink/ts-template/issues"
  },
  "homepage": "https://github.com/smetanink/ts-template#readme",
  "devDependencies": {
    "@types/jest": "29.2.4",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "@vercel/ncc": "0.36.0",
    "env-cmd": "10.1.0",
    "eslint": "8.30.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "npm-check-updates": "16.6.0"
  },
  "dependencies": {
    "pino": "8.8.0"
  }
}
